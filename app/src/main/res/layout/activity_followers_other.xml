<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FollowersOtherActivity">

<LinearLayout
    android:id="@+id/linearLayoutFollowers_Other"
    android:layout_width="412dp"
    android:layout_height="61dp"
    android:background="#00AAB7"
    android:orientation="vertical"
    android:visibility="visible"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

</LinearLayout>

<android.support.constraint.Placeholder
    android:id="@+id/placeholder1_followersOther"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:layout_marginTop="25dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/linearLayoutFollowers_Other" />

<android.support.constraint.Placeholder
    android:id="@+id/placeholder2_followersOther"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:layout_marginTop="8dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/placeholder1_followersOther" />

<android.support.constraint.Placeholder
    android:id="@+id/placeholder3_followersOther"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:layout_marginTop="8dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/placeholder2_followersOther" />

<android.support.constraint.Placeholder
    android:id="@+id/placeholder4_followersOther"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:layout_marginTop="8dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/placeholder3_followersOther" />

<android.support.constraint.Placeholder
    android:id="@+id/placeholder5_followersOther"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:layout_marginTop="8dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/placeholder4_followersOther" />


<ImageButton
    android:id="@+id/backfollowersButtonOther"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginLeft="8dp"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="8dp"
    android:background="#00AAB7"
    app:layout_constraintBottom_toBottomOf="@+id/linearLayoutFollowers_Other"
    app:layout_constraintStart_toStartOf="@+id/linearLayoutFollowers_Other"
    app:layout_constraintTop_toTopOf="@+id/linearLayoutFollowers_Other"
    app:srcCompat="@android:drawable/ic_media_rew" />

<TextView
    android:id="@+id/followersTextOther"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginLeft="8dp"
    android:layout_marginEnd="8dp"
    android:layout_marginRight="8dp"
    android:text="@string/followers_followers"
    android:textColor="#FFFFFF"
    android:textSize="18sp"
    app:layout_constraintBottom_toBottomOf="@+id/linearLayoutFollowers_Other"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="@+id/linearLayoutFollowers_Other" />


</android.support.constraint.ConstraintLayout>